<p id="notice"><%= notice %></p>

<h1>Maheshes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @maheshes.each do |mahesh| %>
      <tr>
        <td><%= mahesh.name %></td>
        <td><%= mahesh.age %></td>
        <td><%= link_to 'Show', mahesh %></td>
        <td><%= link_to 'Edit', edit_mahesh_path(mahesh) %></td>
        <td><%= link_to 'Destroy', mahesh, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mahesh', new_mahesh_path %>
